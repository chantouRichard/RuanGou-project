[36m@@ -26,6 +30,120 @@[m [mnamespace frontend.Views.Pages[m
         {[m
             InitializeComponent();[m
             DataContext = App.host.Services.GetService<QuickLaunchViewModel>();[m
[32m+[m[32m            this.Loaded += SettingsPage_Loaded; // 为页面加载事件添加处理程序[m
[32m+[m[32m        }[m
[32m+[m[32m        // **注册表操作的常量**[m
[32m+[m[32m        // 这是 Windows 存储用户应用程序开机自启动信息的标准注册表路径。[m
[32m+[m[32m        private const string RUN_KEY = "SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Run";[m
[32m+[m[32m        // **非常重要**：请将 "MyDesktopAssistantApp" 替换为您的应用程序的唯一名称。[m
[32m+[m[32m        // 这个名称会显示在 Windows 任务管理器的“启动”选项卡中。[m
[32m+[m[32m        private const string APP_NAME = "MyDesktopAssistantApp";[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// 当设置页面加载完成后触发的事件处理程序。[m
[32m+[m[32m        /// 它用于初始化开机启动复选框的显示状态。[m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        private void SettingsPage_Loaded(object sender, RoutedEventArgs e)[m
[32m+[m[32m        {[m
[32m+[m[32m            // 根据应用程序当前的开机启动状态设置复选框的选中状态。[m
[32m+[m[32m            StartupCheckBox.IsChecked = IsAppSetToRunOnStartup();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// 检查应用程序是否已设置为开机自启动。[m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        /// <returns>如果应用程序已配置为开机启动，则返回 true；否则返回 false。</returns>[m
[32m+[m[32m        public bool IsAppSetToRunOnStartup()[m
[32m+[m[32m        {[m
[32m+[m[32m            // 打开当前用户（HKEY_CURRENT_USER）的 Run 注册表项[m
[32m+[m[32m            using (RegistryKey key = Registry.CurrentUser.OpenSubKey(RUN_KEY))[m
[32m+[m[32m            {[m
[32m+[m[32m                if (key != null)[m
[32m+[m[32m                {[m
[32m+[m[32m                    // 尝试获取以 APP_NAME 为名称的注册表值[m
[32m+[m[32m                    object value = key.GetValue(APP_NAME);[m
[32m+[m[32m                    // 获取当前应用程序的完整执行路径[m
[32m+[m[32m                    string currentAppPath = Assembly.GetExecutingAssembly().Location;[m
[32m+[m
[32m+[m[32m                    // 如果值存在且与当前应用程序的路径匹配（不区分大小写），则表示已设置开机启动。[m
[32m+[m[32m                    if (value != null && value.ToString().Equals(currentAppPath, StringComparison.OrdinalIgnoreCase))[m
[32m+[m[32m                    {[m
[32m+[m[32m                        return true;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// 将应用程序设置为开机自启动。[m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        public void SetAppToRunOnStartup()[m
[32m+[m[32m        {[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                string appPath = Assembly.GetExecutingAssembly().Location;[m
[32m+[m
[32m+[m[32m                // 打开 Run 注册表项，并允许写入 (第二个参数设置为 true)[m
[32m+[m[32m                using (RegistryKey key = Registry.CurrentUser.OpenSubKey(RUN_KEY, true))[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (key != null)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        // 设置注册表值：键名为 APP_NAME，值为应用程序路径[m
[32m+[m[32m                        key.SetValue(APP_NAME, appPath);[m
[32m+[m[32m                        MessageBox.Show("应用程序已成功设置为开机自启。", "设置成功", MessageBoxButton.OK, MessageBoxImage.Information);[m
[32m+[m[32m                    }[m
[32m+[m[32m                    else[m
[32m+[m[32m                    {[m
[32m+[m[32m                        MessageBox.Show("无法访问注册表键。请确保您的应用程序有足够的权限。", "错误", MessageBoxButton.OK, MessageBoxImage.Error);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Exception ex)[m
[32m+[m[32m            {[m
[32m+[m[32m                // 捕获并显示可能发生的任何异常（例如权限问题）。[m
[32m+[m[32m                MessageBox.Show($"设置开机自启失败: {ex.Message}", "错误", MessageBoxButton.OK, MessageBoxImage.Error);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// 取消应用程序的开机自启动设置。[m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        public void DisableAppRunOnStartup()[m
[32m+[m[32m        {[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                // 打开 Run 注册表项，并允许写入 (true)[m
[32m+[m[32m                using (RegistryKey key = Registry.CurrentUser.OpenSubKey(RUN_KEY, true))[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (key != null)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        // 删除对应的注册表值。第二个参数 'false' 表示即使值不存在也不会抛出异常。[m
[32m+[m[32m                        key.DeleteValue(APP_NAME, false);[m
[32m+[m[32m                        MessageBox.Show("应用程序已成功取消开机自启。", "设置成功", MessageBoxButton.OK, MessageBoxImage.Information);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Exception ex)[m
[32m+[m[32m            {[m
[32m+[m[32m                MessageBox.Show($"取消开机自启失败: {ex.Message}", "错误", MessageBoxButton.OK, MessageBoxImage.Error);[m
[32m+[m[32m            }[m
         }[m
[32m+[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// 当开机启动复选框被选中时触发的事件处理程序。[m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        private void StartupCheckBox_Checked(object sender, RoutedEventArgs e)[m
